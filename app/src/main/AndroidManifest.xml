<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.studenttutormatchapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.StudentTutorMatchApp">
        <activity android:name=".FindBidsActivity"></activity>
        <activity android:name=".ListOffersActivity" />
        <activity android:name=".MessageListActivity" />
        <activity android:name=".DashboardActivity" />
        <activity android:name=".BidFormActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.FIT3077.API_KEY"
            android:value="${FIT3077_API_KEY}" />
    </application>

</manifest>